{
  "manifest_version": 2,

  "name": "AutoResponder Chrome",
  "description": "Catch the requests and return response from other urls or local files, block ads",
  "version": "1.5",

  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scripts": ["js/vendor.js", "js/background.js"]
  },

  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "icons": {
    "128": "icon.png"
  }
}